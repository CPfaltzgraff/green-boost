{
  "name": "gboost-infra",
  "version": "0.2.1",
  "description": "Green Boost Infrastructure Library of CDK Constructs",
  "type": "module",
  "exports": {
    "./*": "./src/*.js"
  },
  "publishConfig": {
    "exports": {
      "./*": "./lib/*.js"
    },
    "typesVersions": {
      "*": {
        "*": [
          "lib/*"
        ]
      }
    }
  },
  "files": [
    "lib/**/*"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "node --loader ts-node/esm scripts/build.ts",
    "lint": "eslint \"src/**/*.ts\"",
    "lint-staged": "lint-staged",
    "test": "jest"
  },
  "dependencies": {
    "@aws-cdk/aws-appsync-alpha": "^2.5.0-alpha.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.49.0",
    "aws-cdk-lib": "^2.10.0",
    "cdk-nag": "^2.4.43",
    "constructs": "^10.0.51",
    "esbuild": "^0.14.16",
    "gboost-common": "workspace:^0.2.1",
    "joi": "^17.6.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.92",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.22",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-define-config": "^1.2.3",
    "eslint-plugin-jest": "^25.7.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fs-extra": "^10.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/awslabs/green-boost.git",
    "directory": "packages/gboost-infra"
  },
  "keywords": [
    "aws",
    "serverless",
    "cdk",
    "green",
    "boost",
    "gboost",
    "appsync",
    "constructs"
  ],
  "author": "AWS Professional Service Remote Development Team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/awslabs/green-boost/issues"
  },
  "homepage": "https://awslabs.github.io/green-boost"
}
