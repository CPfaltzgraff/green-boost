{
  "name": "gboost-common",
  "version": "0.2.0",
  "description": "Green Boost Common Library",
  "type": "module",
  "exports": {
    "./*": "./src/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*"
      ]
    }
  },
  "publishConfig": {
    "exports": {
      "./*": "./lib/*.js"
    },
    "typesVersions": {
      "*": {
        "*": [
          "lib/*"
        ]
      }
    }
  },
  "files": [
    "lib/**/*"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "rimraf lib && tsc",
    "lint": "eslint \"src/**/*.ts\"",
    "lint-staged": "lint-staged",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-define-config": "^1.2.3",
    "eslint-plugin-jest": "^25.7.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/awslabs/green-boost.git",
    "directory": "packages/gboost-common"
  },
  "keywords": [
    "aws",
    "serverless",
    "react",
    "cdk",
    "vite",
    "green",
    "boost",
    "gboost"
  ],
  "author": "AWS Professional Service Remote Development Team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/awslabs/green-boost/issues"
  },
  "homepage": "https://awslabs.github.io/green-boost"
}
